    "def contar_palabras(frase):\n",
    "    palabras = frase.split()\n",
    "    return len(palabras)\n",
    "\n",
    "\n",
    "frase_ingresada = input(\"Ingrese una frase: \")\n",
    "\n",
    "\n",
    "cantidad_palabras = contar_palabras(frase_ingresada)\n",
    "print(f\"La frase tiene {cantidad_palabras} palabra(s).\")\n"
   ]
