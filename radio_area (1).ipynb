   "source": [
    "import math\n",
    "\n",
    "def radio_area(d):\n",
    "   \n",
    "    radio = d / 2\n",
    "\n",
    "   \n",
    "    area = math.pi * radio**2\n",
    "\n",
    "   \n",
    "    return area\n",
    "\n",
    "\n",
    "diametro = 10\n",
    "resultado = radio_area(diametro)\n",
    "\n",
    "\n",
    "print(f\"El área del círculo con diámetro {diametro} es: {resultado}\")\n"
   ]
